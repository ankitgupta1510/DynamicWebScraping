package crawlerDesign;


public class ProductData {
    private String category;
    private String name;
    private double priceMRP;

    
    public ProductData(String category, String name,double priceMRP) {
        this.category = category;
        this.name = name;
        this.priceMRP = priceMRP;
    }
    
    
    public String toString() {
        return String.format("Category: %s | Product: %s |MRP: â‚¹ ", category, name, priceMRP);
    }
    
    
    public String getCategory() { return category; }
    public String getName() { return name; }
    public double getPriceMRP() { return priceMRP; }
    
}

