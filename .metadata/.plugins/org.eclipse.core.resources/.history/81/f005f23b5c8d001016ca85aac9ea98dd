package crawlerDesign;

import io.github.millij.poi.ss.model.annotations.Sheet;
import io.github.millij.poi.ss.model.annotations.SheetColumn;

@Sheet
public class ProductData {
	
	@SheetColumn("category")
    private String category;
	
	@SheetColumn("Product name")
    private String name;
	
	@SheetColumn("Product Price")
    private double priceMRP;
	
	@SheetColumn("Weight")
    private double weight;

    
    public ProductData(String category, String name,double priceMRP,double weight) {
        this.category = category;
        this.name = name;
        this.priceMRP = priceMRP;
        this.weight = weight;
        
    }
    
    
    public String toString() {
        return String.format("Category: %s | Product: %s |MRP: â‚¹%.2f |Weight: %.2f", category, name, priceMRP,weight);
    }
    
    
    public String getCategory() { return category; }
    public String getName() { return name; }
    public double getPriceMRP() { return priceMRP; }
    public double getWeight() {return weight;}
    
}

