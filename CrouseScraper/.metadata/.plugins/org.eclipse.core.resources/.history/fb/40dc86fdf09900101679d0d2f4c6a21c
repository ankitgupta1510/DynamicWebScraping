package models;

public class TravelPolicy implements Policy {
    private String planName, insurer, logoUrl, startingAt, personalAccident;

    public TravelPolicy(String planName, String insurer, String logoUrl, String startingAt, String personalAccident) {
        this.planName = planName;
        this.insurer = insurer;
        this.logoUrl = logoUrl;
        this.startingAt = startingAt;
        this.personalAccident = personalAccident;
    }

    @Override
    public String[] toArray() {
        return new String[]{planName, insurer, logoUrl, startingAt, personalAccident};
    }
}